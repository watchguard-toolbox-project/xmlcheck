<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <parser>
        <target>phpdoc.cache</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
	    <target>docs/api</target>
    </transformer>
    <transformations>
        <template name="responsive-twig"/>
    </transformations>
    <files>
        <directory>lib</directory>
        <file>xmlcheck.php</file>
        <ignore>doc</ignore>
        <ignore>vendor</ignore>
    </files>
</phpdoc>
